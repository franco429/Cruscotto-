\par }\pard\plain \qc \li0\ri0\widctlpar\tx-2127\faauto\adjustright\rin0\lin0\itap0 \fs20\lang1040\langfe1040\cgrid\langnp1040\langfenp1040 {\b\fs24\insrsid11891772 
